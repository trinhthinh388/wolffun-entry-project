{
  "name": "wolffun-entry-project",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/trinhthinh388/wolffun-entry-project",
  "author": "Thinh Trinh",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack/webpack.config.js --port 3000",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config config/webpack/webpack.config.js",
    "prepare": "husky install",
    "lint:code": "eslint src --ext .js,.jsx,.ts,.tsx && stylelint --allow-empty-input \"src/**/*.(js|jsx|ts|tsx)\" && prettier src --check",
    "lint:css": "stylelint --config stylelint.css.config.js --allow-empty-input \"src/**/*.(css|scss)\"",
    "lint": "yarn lint:code && yarn lint:css",
    "typecheck": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "@fontsource/oxanium": "^4.5.5",
    "axios": "^0.26.1",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "6",
    "react-spring": "^9.4.4",
    "react-use-measure": "^2.1.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-async-to-generator": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@svgr/webpack": "^6.2.1",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/react": "^18.0.1",
    "@types/react-dom": "18.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.2",
    "react-app-polyfill": "^3.0.0",
    "react-test-renderer": "^18.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "tailwindcss": "^3.0.23",
