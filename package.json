    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --ext .js,.jsx,.ts,.tsx --fix",
      "stylelint"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --config stylelint.css.config.js"
    ]
  }
}
